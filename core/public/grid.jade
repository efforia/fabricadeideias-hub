each object in f
  if object.token == '^'
    .span2.block(style='display: none;')
      a.back(href='#', style='float: left; color: black; height:100%; width: 100%;')
        div(style='background: black; height: 100%; top: 0%; display: block; text-align: center;')
          img(src='#{static_url}img/spin.png', style='height:100%')
  if object.token == '*'
    .span2.block(style='background:black; color:white;')
      if object.action == 'create'
        a(href='#{object.href}').creation
          .details(style='text-align: center;')
            h2
              | Adicionar
              .title(style='display: inline;') 
                #{object.vals.quantity} Itens
            p
              .description(style='display: inline;') 
                #{object.vals.value} Créditos
      if object.action == 'buy'
        a(href='#{object.href}').purchase
          .details(style='text-align: center;')
            h2
              | Comprar
              .title(style='display: inline;') 
                #{object.vals.quantity} Itens
            p
              .description(style='display: inline;') 
                #{object.vals.value} Créditos
      if object.action == 'play'
        a(href='#{object.href}').play
          .details(style='text-align: center;')
            h2
              | Tocar
              .title(style='display: inline;') 
                #{object.vals.quantity} Itens
            p
              .description(style='display: inline;') 
                #{object.vals.value} Créditos
      if object.action == 'new'
        a(href='#{object.href}').new
          .details(style='text-align: center;')
            h2
              | Selecionar novos
              .title(style='display: inline;') 
                #{object.vals.quantity} Itens
            p
              .description(style='display: inline;') 
                #{object.vals.value} Créditos
      if object.action == 'selection'
        a(href='#{object.href}').selection(style="color:white; top:0%; display:block;")
          .details(style='text-align: center;')
            h2
              | Selecionar
              .title(style='display: inline;') 
                #{object.vals.quantity} Itens
            p
              .description(style='display: inline;') 
                #{object.vals.value} Créditos
  if object.token == '!#'
    .span4.block.page(style='background:#F9A600')
      a(href='#')
        div(style='top: 0%; display: block;')
          .content(style='padding:5px;')
            h2
              #{object.name}
            {{ object.content|safe }}
          .id(style='display:none;')
            #{object.id}
          .time(style='display: none;') #{object.date};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.token == '!%'
    .span3.block.image(style='background:#741B47')
      a(href='#')
        div(style='top: 0%; display: block;')
          .content(style='padding:5px;')
                  h2(style='color:white')
                          #{object.link}
            //img(src='#{object.link}',style='width:100%;height:100%')
          .id(style='display:none;')
            #{object.id}
          .time(style='display: none;') #{object.date};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.token == '++'
    .span2.block.fade
      a(href='#')
        .mosaic(style='top: 0%; display: block;')
          .details(style='text-align: center;')
            h2
              if object.quantity != 1 
                #{object.quantity} #{object.product_trimmed}
            p #{object.total_value} Créditos
            .time(style='display: none;') #{object.product.date}
          .date #{object.product.date.day} de #{object.product_month}
      div(style='top: 0; background: white; text-align: center;')
        if object.product.visual != true
          img(src='#{static_url}img/playit.png', height='100%')
        else
          img(src='#{object.product.visual}', height='100%')
  if object.token == '&'
    .span2.block.fade
      a(href='#')
        .product.mosaic(style='top: 0%; display: block;')
          .details
            h2 #{object.name_trimmed}
            .time(style='display: none;') #{object.date}
          .date #{object.date.day} de #{object.real_month}
      div(style='top: 0; background: white; text-align: center;')
        if object.visual != true
          img(src='#{static_url}img/playit.png', height='100%')
        else
          img(src='#{object.visual}', height='100%')
  if object.token == '@!'
    .span2.block(style='background: #E9E900')
      a(href='#')
        .spreadablespread(style='color: black; top: 0%; display: block;')
          .details
            | #{object.content_trimmed}...
            .time(style='display: none;') #{object.date};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.token == '@$'
    .span2.block(style='background: #F50000')
      a(href='#')
        .eventspread(style='top: 0%; display: block;')
          .details
            #{object.name_trimmed}
            .time(style='display: none;') #{object.date};#{object.token}@
          .date #{object.date.day} de #{object.month}
  if object.token == '!'
    .span2.block(style='background:#00A946; color:white;')
      a(href='#')
        .spreadable(style='top: 0%; display: block;')
          .content(style='padding:5px;')
            {{ object.content|safe }}
          .id(style='display:none;')
            #{object.id}
          .time(style='display: none;') #{object.date};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.token == '@@'
    .span3.block(style='background: #F50000')
      a(href='#')
        .event(style='color: white; top: 0%; display: block;')
          .details
            h2 #{object.name_trimmed} em #{object.location}
            .time(style='display: none;') #{object.date};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.token == '>>'
    .span2.block(style='background: #6D1943')
      a(href='#')
        .schedule(href='schedule', style='color: white; top: 0%; display: block;')
          .details
            h2 #{object.name_trimmed}
            .time(style='display: none;') #{object.date};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.etoken == '>'
    .span3.block(style='background: #1364F0')
      a(href='#{object.token}')
        .playable(style='display: block; height:100%;')
          .action(style='z-index:-1;')
            if object.visual == true
                img(src='#{object.visual}', height='100%', width='100%')
            else
                h2 #{object.name_trimmed}
            .time(style='display: none;') #{object.id};#{object.etoken}
            .date #{object.date.day} de #{object.month}
  if object.token == '##'
    .span2.block(style='background: #1364F0')
      a(href='#')
        .movement(href='movement?view=#{object.name}', style='color: white; top: 0%; display: block;')
          .details
            h2 #{object.name_trimmed}
            .time(style='display: none;') #{object.date};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.token == '@#'
    .span2.block(style='background: #DEDEDE')
      a(href='#')
        .causablespread(style='color: black; top: 0%; display: block;')
          .details
            | ##{object.name_trimmed}
            .time(style='display: none;') #{object.date};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.token == '#'
    .span2.block
      a(href='#{object.play.token}')
        .causable(style='display: block; height:100%;')
          .action(style='z-index:-1; background: #F50000')
            if object.play.visual == true
              img(src='#{object.visual}', height='100%', width='100%')
          .details
            h2 #{object.trim}
            .time(style='display: none;') #{object.id};#{object.token}
          .date #{object.date.day} de #{object.month}
  if object.token == '%%'
    .span2.block-transparent
  if object.token == ''
    .span1.block.profile(style='background:black')
      a(href='#',style='display: block; height:100%;')
        .action(style='z-index:-1;');
          if object.visual == true
            img(src='#{object.visual}', height='100%', width='100%')
        .details 
          .name(style='display: none') #{object.id}
          h2(style='color:white') #{object.user.first_name}
        .date #{object.date.day} de #{object.month}
